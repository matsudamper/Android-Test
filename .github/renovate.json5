{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 100,
  "gradle": {
    "fileMatch": [
      "\\.gradle(\\.kts)?$",
      "(^|/)gradle\\.properties$",
      "(^|/)gradle/.+\\.toml$",
      "(^|/)buildSrc/.+\\.kt$",
      "(^|/)VersionCatalog\\.kt$",
      "\\.versions\\.toml$",
      "(^|/)versions.props$",
      "(^|/)versions.lock$"
    ],
  }
}
